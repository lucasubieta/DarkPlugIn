{
    "manifest_version": 3,
    "name": "Modo Oscuro Simplificado",
    "version": "1.0",
    "description": "Activa el modo oscuro en cualquier p√°gina web.",
    "permissions": ["storage", "scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": ["<all_urls>"]
  }
  